{
  "name": "dahabiyat-nile-cruise-mobile",
  "version": "3.0.0",
  "description": "Dahabiyat Nile Cruise Mobile App - Ocean Blue Theme",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "start-expo": "expo start",
    "android": "react-native run-android",
    "android-expo": "expo run:android",
    "ios": "react-native run-ios",
    "ios-expo": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:android:aab": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "clean": "react-native start --reset-cache",
    "clean-android": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "@expo/metro-config": "~0.18.11",
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.3",
    "@react-native-community/cli": "^19.1.1",
    "@react-native-community/cli-plugin-metro": "^12.3.6",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo": "~51.0.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-modules-core": "~1.12.26",
    "expo-status-bar": "~1.12.1",
    "metro": "~0.80.8",
    "metro-config": "^0.83.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.13",
    "react-native-paper": "^5.12.3",
    "react-native-vector-icons": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "typescript": "^5.1.3"
  },
  "keywords": [
    "react-native",
    "expo",
    "dahabiyat",
    "nile-cruise",
    "egypt",
    "tourism",
    "mobile-app",
    "ocean-blue-theme"
  ],
  "author": "Dahabiyat Nile Cruise Team",
  "license": "MIT",
  "private": true
}
